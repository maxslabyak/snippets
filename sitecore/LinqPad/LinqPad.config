        <handler type="Sitecore.ContentSearch.Maintenance.IndexDependentHtmlCacheManager, Sitecore.ContentSearch" method="Clear"/>
      </event>
      <event name="indexing:end:remote" patch:source="Sitecore.ContentSearch.config"/>
